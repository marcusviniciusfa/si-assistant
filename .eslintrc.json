{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "google",
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "valid-jsdoc": "off",
    "require-jsdoc": "off",
    "new-cap": "off",
    "max-len": "off",
    "guard-for-in": "off"
  }
}